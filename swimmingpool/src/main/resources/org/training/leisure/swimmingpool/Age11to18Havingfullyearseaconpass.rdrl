package org.training.leisure.swimmingpool;

import java.lang.Number;

rule "Age11to18Havingfullyearseaconpass"
	dialect "mvel"
	when
		Person( age >= 11 )
		Person( age <= 18 )
		subscription( type matches "full year" )
	then
		Quote fact0 = new Quote();
		fact0.setPriceList( "110" );
		insertLogical( fact0 );
end
